#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/skkylimits/Heaven/WSL2/node_modules/.pnpm/shellcheck@1.1.0/node_modules/shellcheck/shellcheck-stable/node_modules:/home/skkylimits/Heaven/WSL2/node_modules/.pnpm/shellcheck@1.1.0/node_modules/shellcheck/node_modules:/home/skkylimits/Heaven/WSL2/node_modules/.pnpm/shellcheck@1.1.0/node_modules:/home/skkylimits/Heaven/WSL2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/skkylimits/Heaven/WSL2/node_modules/.pnpm/shellcheck@1.1.0/node_modules/shellcheck/shellcheck-stable/node_modules:/home/skkylimits/Heaven/WSL2/node_modules/.pnpm/shellcheck@1.1.0/node_modules/shellcheck/node_modules:/home/skkylimits/Heaven/WSL2/node_modules/.pnpm/shellcheck@1.1.0/node_modules:/home/skkylimits/Heaven/WSL2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/sh" ]; then
  exec "$basedir/sh"  "$basedir/../shellcheck/shellcheck-stable/shellcheck" "$@"
else
  exec sh  "$basedir/../shellcheck/shellcheck-stable/shellcheck" "$@"
fi
